{
  "name": "blog-ap",
  "version": "1.2.0",
  "description": "Blog API Written in Node.js, Express, MongoDB, JSON Web Token (JWT) Authentication, Mongoose, Middleware Authentication.",
  "main": "src/index.ts",
  "scripts": {
    "nodemon": "nodemon",
    "prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/src/index.js",
    "dev": "nodemon -r ts-node/register/transpile-only -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc -p tsconfig.prod.json",
    "lint": "eslint --fix",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run test",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/src/index.js",
    "postinstall": "tsc"
  },
  "engines": {
    "node": "v14.17.0"
  },
  "keywords": [],
  "author": "Saddam Arbaa",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saddamarbaa/blog-api.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.1.2",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "helmet": "^5.1.1",
    "http-errors": "^2.0.0",
    "joi": "^17.6.3",
    "joi-objectid": "^4.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.10.0",
    "mongoose": "^6.6.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.8.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "supertest": "^6.2.4",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4",
    "validator": "^13.7.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.31",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.0",
    "@types/nodemailer": "^6.4.6",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.7",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1"
  }
}
